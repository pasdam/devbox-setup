{
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "dart.env": {
        "ENABLE_FLUTTER_DESKTOP": true,
    },
    "dart.warnWhenEditingFilesOutsideWorkspace": false,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "all",
    "editor.rulers": [80,120],
    "editor.suggestSelection": "first",
    "explorer.autoReveal": false,
    "files.exclude": {
        "**/.git": false
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "go.formatTool": "goimports",
    "go.useLanguageServer": true,
    "java.configuration.checkProjectSettingsExclusions": false,
    "plantuml.render": "PlantUMLServer",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "vs-kubernetes": {
        "vs-kubernetes.helm-path": "/Users/paco/.vs-kubernetes/tools/helm/darwin-amd64/helm",
        "vs-kubernetes.kubectl-path": "/Users/paco/.vs-kubernetes/tools/kubectl/kubectl",
        "vs-kubernetes.minikube-path": "/Users/paco/.vs-kubernetes/tools/minikube/darwin-amd64/minikube",
        "vs-kubernetes.draft-path": "/Users/paco/.vs-kubernetes/tools/draft/darwin-amd64/draft",
        "vs-kubernetes.minikube-path.mac": "/Users/paco/.vs-kubernetes/tools/minikube/darwin-amd64/minikube"
    },
    "workbench.startupEditor": "newUntitledFile",
    "todo-tree.tree.showScanModeButton": false,
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.coverOnSingleTest": true,
    "go.coverOnSingleTestFile": true,
    "go.testTimeout": "10s",
    "go.buildFlags": [
        "-gcflags=-l"
    ],
}
