alias dip='docker image prune'
alias dl='docker logs -f'
alias dnl='docker network ls'
alias dnp='docker network prune'
alias docker_jupyter='docker run --rm -d -p 11000:8888 -v "$HOME/workspace/jupyter":/home/jovyan/work --name jupyter jupyter/datascience-notebook:1386e2046833 && docker logs -f jupyter'
